{
    "id" : "xyz.safeworlds.hiit.Devel",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "49",
    "sdk" : "org.gnome.Sdk",
    "command" : "hiit",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--socket=pulseaudio",
        "--device=dri"
    ],
    "separate-locales": false,
    "modules" : [
        {
            "name" : "hiit",
            "buildsystem" : "meson",
            "config-opts" : [
                "-Dprofile=development"
            ],
            "sources" : [
                {
                    "type" : "dir",
                    "path" : "../"
                }
            ]
        }
    ]
}
