#: src/setup.rs:11
msgid "Exercise Timer"
msgstr "Edzésóra"

#: data/xyz.safeworlds.hiit.desktop.in.in:4
msgid "Timer clock for high intensity interval training"
msgstr "Időzítő intenzív intervallumedzéshez"

